echo Writing to phase switch control registers;
ibob i01.fxc regwrite phsw/control 0x27531;
ibob i02.fxc regwrite phsw/control 0x26420;
ibob i03.fxc regwrite phsw/control 0x27531;
ibob i04.fxc regwrite phsw/control 0x26420;
ibob i05.fxc regwrite phsw/control 0x27531;
ibob i06.fxc regwrite phsw/control 0x26420;
ibob i07.fxc regwrite phsw/control 0x27531;
ibob i08.fxc regwrite phsw/control 0x26420;
ibob i09.fxc regwrite phsw/control 0x27531;
ibob i10.fxc regwrite phsw/control 0x26420;
ibob i11.fxc regwrite phsw/control 0x27531;
ibob i12.fxc regwrite phsw/control 0x26420;
ibob i13.fxc regwrite phsw/control 0x27531;
ibob i14.fxc regwrite phsw/control 0x26420;
ibob i15.fxc regwrite phsw/control 0x27531;
ibob i16.fxc regwrite phsw/control 0x26420;

echo Reading back phase switch control registers;
ibob i01.fxc regread phsw/control | awk '{print "i01.fxc", $1}';
ibob i02.fxc regread phsw/control | awk '{print "i02.fxc", $1}';
ibob i03.fxc regread phsw/control | awk '{print "i03.fxc", $1}';
ibob i04.fxc regread phsw/control | awk '{print "i04.fxc", $1}';
ibob i05.fxc regread phsw/control | awk '{print "i05.fxc", $1}';
ibob i06.fxc regread phsw/control | awk '{print "i06.fxc", $1}';
ibob i07.fxc regread phsw/control | awk '{print "i07.fxc", $1}';
ibob i08.fxc regread phsw/control | awk '{print "i08.fxc", $1}';
ibob i09.fxc regread phsw/control | awk '{print "i09.fxc", $1}';
ibob i10.fxc regread phsw/control | awk '{print "i10.fxc", $1}';
ibob i11.fxc regread phsw/control | awk '{print "i11.fxc", $1}';
ibob i12.fxc regread phsw/control | awk '{print "i12.fxc", $1}';
ibob i13.fxc regread phsw/control | awk '{print "i13.fxc", $1}';
ibob i14.fxc regread phsw/control | awk '{print "i14.fxc", $1}';
ibob i15.fxc regread phsw/control | awk '{print "i15.fxc", $1}';
ibob i16.fxc regread phsw/control | awk '{print "i16.fxc", $1}';
