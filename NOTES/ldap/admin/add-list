# user1 data disk
dn: cn=user1,nisMapName=auto.data,ou=AUTOFS,dc=obs,dc=hcro,dc=org
objectClass: nisObject
nisMapName: auto.data
cn: user1
nisMapEntry: -fstype=nfs,hard,intr,root_squash,rsize=8129,wsize=8129 user1.hcro.org:/exports/user1

